<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.he.attenddemo.dao.IUserDao">
    <!-- 查询 -->
    <select id="loadById" resultType="com.he.attenddemo.model.User">
       select * from user where id = #{id}
    </select>

    <select id="findAll" resultType="com.he.attenddemo.model.User">
      select * from user
    </select>

</mapper>